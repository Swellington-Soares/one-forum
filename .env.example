# Database Configuration (Railway MySQL)
MYSQL_URL=jdbc:mysql://your-host:port/database
MYSQL_USER=root
MYSQL_PASSWORD=your-password

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_KEY=your-supabase-key
SUPABASE_BUCKET=uploads

# JWT Configuration (IMPORTANTE: Gere novos secrets para produção!)
JWT_KEY_SECRET=your-secret-key
JWT_REFRESH_KEY_SECRET=your-refresh-secret-key

# API URLs (Configure no Render)
API_BASE_URL=https://your-app-name.onrender.com
FRONTEND_URL=https://your-frontend-url.com

# Mail Configuration (opcional)
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587

# Swagger (deixe false em produção)
SWAGGER_ENABLED=false
